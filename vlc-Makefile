all:
	true

install:
	install apply_extra /app/bin
	install vlc.sh /app/bin
	cp /usr/bin/ar /app/bin
	cp /usr/lib/libbfd-*.so /app/lib

	install -D org.videolan.App.png /app/share/icons/hicolor/128x128/apps/org.videolan.App.png
	install -D org.videolan.App.desktop /app/share/applications/org.videolan.App.desktop
	install -D org.videolan.App.appdata.xml /app/share/appdata/org.videolan.App.appdata.xml
